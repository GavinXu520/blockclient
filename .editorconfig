# editorconfig.org
root = true

[*]
indent_style = tab
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,json,html,ejs,tpl,jsx,vue,less,yaml}]
indent_style = space
indent_size = 2

[*.json]
insert_final_newline = false

[*.md]
trim_trailing_whitespace = false

[*.styl]
indent_style = tab

[*.go]
indent_style = tab
indent_size = 8
